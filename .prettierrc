{
	"trailingComma": "all",
	"singleQuote": true,
	"useTabs": true,
	"overrides": [
		{
			"files": [".prettierrc", ".sgcrc"],
			"options": {
				"parser": "json"
			}
		},
		{
			"files": "package.json",
			"options": {
				"useTabs": false
			}
		},
		{
			"files": "**/*.yml",
			"options": {
				"useTabs": false
			}
		}
	]
}

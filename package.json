{
  "name": "vue-mixin-decorator",
  "version": "0.0.0-development",
  "description": "Mixin decorators for Vue Component",
  "main": "dist/vue-mixin-decorator.umd.js",
  "keywords": [
    "vue",
    "typescript",
    "decorator"
  ],
  "author": "justrhysism",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -p src/tsconfig.json",
    "build:umd": "rollup -c",
    "pretest": "tsc",
    "test": "ava",
    "prepublish": "yarn test && yarn run build",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ],
    "files": [
      "test/*.spec.js"
    ],
    "source": [
      "lib/**/*.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "files": [
    "lib",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^8.0.47",
    "@types/sinon": "^4.1.3",
    "@vue/test-utils": "^1.0.0-beta.10",
    "ava": "^0.24.0",
    "browser-env": "^3.2.4",
    "npm-run-all": "^4.1.2",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "rollup": "^0.54.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "semantic-release": "^12.2.4",
    "sinon": "^4.2.0",
    "typescript": "^2.6.1",
    "vue": "^2.5.13",
    "travis-deploy-once": "^4.3.2"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "vue-class-component": "^6.1.0"
  },
  "peerDependencies": {
    "typescript": "^2.0.0",
    "vue": "^2.5.0"
  },
  "repository": "git+https://github.com/justrhysism/vue-mixin-decorator.git",
  "bugs": {
    "url": "https://github.com/justrhysism/vue-mixin-decorator/issues"
  },
  "homepage": "https://github.com/justrhysism/vue-mixin-decorator#readme",
  "private": false
}
